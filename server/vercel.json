{
    "version": 2,
    "name": "adyamoyee-store",
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "index.js"
        }
    ],
    "env": {
        "MONGO_URI": "@mongo_uri",
        "FRONTEND_URL": "@frontend_url",
        "PORT": "8080"
    }
}
